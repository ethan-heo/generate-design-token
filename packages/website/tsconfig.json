{
	"compilerOptions": {
		"lib": ["ES2017", "DOM", "DOM.Iterable"],
		"strict": true,
		"noImplicitAny": false,
		"jsx": "react",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"resolveJsonModule": true
	},
	"include": ["src/**/*"]
}
