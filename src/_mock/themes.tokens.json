{
	"grid": {
		"mobile": {
			"landscape": {
				"column": {
					"$type": "string",
					"$value": "calc((100% - {spacing.8x.3} * 5) / 4)"
				},
				"row": {
					"$type": "dimension",
					"$value": "{spacing.8x.1}"
				},
				"gutter": {
					"$type": "dimension",
					"$value": "{spacing.8x.3}"
				},
				"margin": {
					"$type": "dimension",
					"$value": "{spacing.8x.3}"
				}
			},
			"portrait": {
				"column": {
					"$type": "string",
					"$value": "calc((100% - {spacing.8x.2} * 5) / 4)"
				},
				"row": {
					"$type": "dimension",
					"$value": "{spacing.8x.1}"
				},
				"gutter": {
					"$type": "dimension",
					"$value": "{spacing.8x.2}"
				},
				"margin": {
					"$type": "dimension",
					"$value": "{spacing.8x.2}"
				}
			}
		},
		"tablet": {
			"column": {
				"$type": "string",
				"$value": "calc((100% - {spacing.8x.3} * 9) / 8)"
			},
			"row": {
				"$type": "dimension",
				"$value": "{spacing.8x.1}"
			},
			"gutter": {
				"$type": "dimension",
				"$value": "{spacing.8x.3}"
			},
			"margin": {
				"$type": "dimension",
				"$value": "{spacing.8x.3}"
			}
		},
		"desktop": {
			"column": {
				"$type": "dimension",
				"$value": "{spacing.8x.9}"
			},
			"row": {
				"$type": "dimension",
				"$value": "{spacing.8x.1}"
			},
			"gutter": {
				"$type": "dimension",
				"$value": "{spacing.8x.3}"
			},
			"margin": {
				"$type": "string",
				"$value": "auto"
			}
		}
	},
	"responsive": {
		"mobile": {
			"landscape": {
				"min-width": {
					"$type": "dimension",
					"$value": "{breakpoint.2}"
				},
				"max-width": {
					"$type": "dimension",
					"$value": "{breakpoint.3}"
				}
			},
			"portrait": {
				"max-width": {
					"$type": "dimension",
					"$value": "{breakpoint.1}"
				}
			}
		},
		"tablet": {
			"min-width": {
				"$type": "dimension",
				"$value": "{breakpoint.4}"
			},
			"max-width": {
				"$type": "dimension",
				"$value": "{breakpoint.5}"
			}
		},
		"desktop": {
			"min-width": {
				"$type": "dimension",
				"$value": "{breakpoint.6}"
			},
			"max-width": {
				"$type": "dimension",
				"$value": "{breakpoint.7}"
			}
		},
		"desktop-large": {
			"min-width": {
				"$type": "dimension",
				"$value": "{breakpoint.8}"
			}
		}
	},
	"text-size": {
		"2-extra-large": {
			"$type": "string",
			"$value": "normal {font-size.rem.8}/{line-height.140} {font-family.default}"
		},
		"extra-large": {
			"$type": "string",
			"$value": "normal {font-size.rem.6}/{line-height.140} {font-family.default}"
		},
		"large": {
			"$type": "string",
			"$value": "normal {font-size.rem.4}/{line-height.140} {font-family.default}"
		},
		"medium": {
			"$type": "string",
			"$value": "normal {font-size.rem.3}/{line-height.120} {font-family.default}"
		},
		"small": {
			"$type": "string",
			"$value": "normal {font-size.rem.2}/{line-height.120} {font-family.default}"
		},
		"extra-small": {
			"$type": "string",
			"$value": "normal {font-size.rem.1}/{line-height.120} {font-family.default}"
		}
	},
	"text-weight": {
		"bold": {
			"$type": "fontWeight",
			"$value": "{font-weight.700}"
		},
		"medium": {
			"$type": "fontWeight",
			"$value": "{font-weight.500}"
		},
		"light": {
			"$type": "fontWeight",
			"$value": "{font-weight.300}"
		}
	},
	"color": {
		"primary": {
			"$type": "color",
			"$value": "{color.vintage.orange}"
		},
		"secondary": {
			"$type": "color",
			"$value": "{color.vintage.black.11}"
		},
		"accent": {
			"$type": "color",
			"$value": "{color.vintage.purple}"
		},
		"success": {
			"$type": "color",
			"$value": "{color.vintage.blue}"
		},
		"warning": {
			"$type": "color",
			"$value": "{color.vintage.yellow}"
		},
		"error": {
			"$type": "color",
			"$value": "{color.vintage.red}"
		},
		"text": {
			"$type": "color",
			"$value": "{color.vintage.black.2}"
		},
		"text-white": {
			"$type": "color",
			"$value": "{color.white}"
		},
		"background": {
			"$type": "color",
			"$value": "{color.white}"
		}
	},
	"border": {
		"thin": {
			"$type": "string",
			"$value": "{border-width.1x.1} {border-style.solid} transparent"
		},
		"light": {
			"$type": "string",
			"$value": "{border-width.1x.2} {border-style.solid} transparent"
		},
		"medium": {
			"$type": "string",
			"$value": "{border-width.1x.3} {border-style.solid} transparent"
		},
		"bold": {
			"$type": "string",
			"$value": "{border-width.1x.4} {border-style.solid} transparent"
		},
		"heavy": {
			"$type": "string",
			"$value": "{border-width.1x.5} {border-style.solid} transparent"
		},
		"{color}": {
			"thin": {
				"$type": "string",
				"$value": "{border-width.1x.1} {border-style.solid} {$value}"
			},
			"light": {
				"$type": "string",
				"$value": "{border-width.1x.2} {border-style.solid} {$value}"
			},
			"medium": {
				"$type": "string",
				"$value": "{border-width.1x.3} {border-style.solid} {$value}"
			},
			"bold": {
				"$type": "string",
				"$value": "{border-width.1x.4} {border-style.solid} {$value}"
			},
			"heavy": {
				"$type": "string",
				"$value": "{border-width.1x.5} {border-style.solid} {$value}"
			}
		}
	},
	"border-radius": {
		"small": {
			"$type": "string",
			"$value": "{border-radius.4x.1}"
		},
		"medium": {
			"$type": "string",
			"$value": "{border-radius.4x.2}"
		},
		"large": {
			"$type": "string",
			"$value": "{border-radius.4x.3}"
		},
		"round": {
			"$type": "string",
			"$value": "{border-radius.full}"
		}
	},
	"spacing": {
		"{spacing.8x}": {
			"$type": "dimension",
			"$value": "{$value}"
		}
	},
	"shadow": {
		"thin": {
			"$type": "string",
			"$value": "{box-shadow.low.bottom-right} {color.vintage.black.2}"
		}
	}
}
