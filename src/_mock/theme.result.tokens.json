{
  "grid": {
    "mobile": {
      "landscape": {
        "column": {
          "$type": "string",
          "$value": "calc((100% - 24px * 5) / 4)"
        },
        "row": {
          "$type": "dimension",
          "$value": "8px"
        },
        "gutter": {
          "$type": "dimension",
          "$value": "24px"
        },
        "margin": {
          "$type": "dimension",
          "$value": "24px"
        }
      },
      "portrait": {
        "column": {
          "$type": "string",
          "$value": "calc((100% - 16px * 5) / 4)"
        },
        "row": {
          "$type": "dimension",
          "$value": "8px"
        },
        "gutter": {
          "$type": "dimension",
          "$value": "16px"
        },
        "margin": {
          "$type": "dimension",
          "$value": "16px"
        }
      }
    },
    "tablet": {
      "column": {
        "$type": "string",
        "$value": "calc((100% - 24px * 9) / 8)"
      },
      "row": {
        "$type": "dimension",
        "$value": "8px"
      },
      "gutter": {
        "$type": "dimension",
        "$value": "24px"
      },
      "margin": {
        "$type": "dimension",
        "$value": "24px"
      }
    },
    "desktop": {
      "column": {
        "$type": "dimension",
        "$value": "72px"
      },
      "row": {
        "$type": "dimension",
        "$value": "8px"
      },
      "gutter": {
        "$type": "dimension",
        "$value": "24px"
      },
      "margin": {
        "$type": "string",
        "$value": "auto"
      }
    }
  },
  "responsive": {
    "mobile": {
